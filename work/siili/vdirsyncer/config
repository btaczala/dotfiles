# vim: ft=config

[general]
status_path = "~/.vdirsyncer/status/"

# CALDAV
[pair siili_calendar]
a = "siili_local"
b = "siili_remote"
collections = ["from a", "from b"]
metadata = ["displayname", "color"]

[storage siili_local]
type = "filesystem"
path = "~/.calendars/siili/"
fileext = ".ics"

[storage siili_remote]
type = "caldav"
url = "http://localhost:1080/users/bartosz.taczala@siili.com/calendar/"
username = "bartosz.taczala@siili.com"
password.fetch = ["command", "/usr/bin/pass", "siili/domain"]

[pair qt_calendar]
a = "qt_local"
b = "qt_remote"
collections = ["from a", "from b"]

[storage qt_local]
type = "filesystem"
path = "~/.calendars/qt/"
fileext = ".ics"

[storage qt_remote]
type = "caldav"
url = "http://localhost:1080/users/bartosz.taczala@qt.io/calendar/"
username = "bartosz.taczala@qt.io"
password.fetch = ["command", "~/dotfiles/work/siili/get_qt_pass.sh"]

