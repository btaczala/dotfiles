URxvt*font: xft:Terminess Powerline:pixelsize=10
URxvt*scrollBar: false
URxvt.saveLines: 32767
URxvt*termName: rxvt
URxvt.perl-ext-common: default,clipboard,keyboard-select
URxvt.iso14755: false
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.Control-Escape: perl:keyboard-select:activate

!-------------------------------------------------------------------------------
! Colors

*background: #181818
*foreground: #c0c0c0
*cursorColor: #93a1a1

!black
*color0:    #252525
*color8:    #454545
!red
*color1:     #FF9F95
*color9:     #FF8D80
!green
*color2:    #A6E22E
*color10:   #B6E354
!yellow
*color3:    #FD971F
*color11:   #FD971F
!blue
*color4:    #435e87
*color12:   #587aa4
!magenta
*color5:    #789ec6
*color13:   #46a4ff
!cyan
*color6:    #5E7175
*color14:   #A3BABF
!white
*color7:    #DBDCDC
*color15:  #FDFDFD

rofi.width:           75
rofi.lines:           10
rofi.columns:         1
rofi.font:            Terminess Powerline 12
rofi.fg:              #ccb18b
rofi.bg:              #282828
rofi.hlfg:            #000000
rofi.hlbg:            #4c78c9
rofi.bc:              #282828
rofi.bw:              1
rofi.loc:             0
rofi.padding:         20
rofi.yoffset:         0
rofi.xoffset:         0
